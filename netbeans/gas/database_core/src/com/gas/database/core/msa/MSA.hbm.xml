<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.gas.domain.core.msa">

    <class name="MSA">
        <id name="hibernateId" column="msaId">
            <generator class="uuid" />
        </id>
        <property name="name" />
        <property name="read" />
        <property name="type" />
        <property name="length" />
        <property name="subMatrix" />
        <property name="lastModifiedDate"/>
        <property name="desc" type="text"/>
        <property name="prevFolderPath" type="text"/>
        <property name="newickStr" type="text"/>
                
        <many-to-one name="richConsensus" lazy="proxy" cascade="all" column="richConsensusId" not-null="false" unique="true"/>
                         
        <many-to-one name="folder" 
                     column="folderId"
                     not-null="true" class="com.gas.domain.core.filesystem.Folder"/>    
        
        <many-to-one name="consensusParam" lazy="proxy" cascade="all" column="consensusParamId" not-null="true" unique="true"/>         
        
        <many-to-one name="seqLogoParam" lazy="proxy" cascade="all" column="seqLogoParamId" not-null="false" unique="true"/>
        
        <many-to-one name="msaSetting" lazy="proxy" cascade="all" column="msaSettingId" not-null="false" unique="true"/>
                     
        <set name="entries" cascade="all,delete-orphan" lazy="true">
            <key column="msaId" />
            <many-to-many column="entryId" unique="true" class="MSA$Entry" />
        </set>  
        
        <set name="clustalwParams" cascade="all,delete-orphan" lazy="true">
            <key column="msaId" />
            <many-to-many column="msaParamId" unique="true" class="com.gas.domain.core.msa.clustalw.ClustalwParam" />
        </set> 
        
        <set name="muscleParams" cascade="all,delete-orphan" lazy="true">
            <key column="msaId" />
            <many-to-many column="muscleParamId" unique="true" class="com.gas.domain.core.msa.muscle.MuscleParam" />
        </set>                                                                                                              
        
        <set name="vfMsaParams" cascade="all,delete-orphan" lazy="true">
            <key column="msaId" />
            <many-to-many column="vfmsaParamId" unique="true" class="com.gas.domain.core.msa.vfmsa.VfMsaParam" />
        </set>
        
    </class>
    
    <class name="MSA$Entry">
        <id name="hibernateId" column="entryId">
            <generator class="increment" />
        </id>        
        <property name="name" />
        <property name="data" type="text"/>
        <!--set name="fetures" cascade="all" lazy="true">
            <key column="entryId" />
            <many-to-many column="fetureId" unique="true" class="com.gas.domain.core.as.Feture" />
        </set-->        
    </class>
    
    <class name="MSASetting">
        <id name="hibernateId" column="msaSettingId">
            <generator class="increment" />
        </id>
        <property name="zoom"/>
        <property name="coverageHeight"/>
        <property name="qualityHeight"/>
        <property name="seqLogoHeight"/>
        
        <property name="coverageDisplay"/>
        <property name="qualityDisplay"/>
        <property name="seqLogoDisplay"/>
        
        <property name="treeEdgeTransform"/>
        <property name="treeForm"/>
        <property name="treeEdgeLabelDisplay"/>
        
        <property name="nodeLabelDisplay"/>
        <property name="sigDigits"/>
        <property name="lineWidth"/>
                
        <property name="selectedTab"/>
        
    </class>
    
    <class name="ConsensusParam">
        <id name="hibernateId" column="consensusParamId">
            <generator class="increment" />
        </id>        
        <property name="ignoreGaps" />
        <property name="plurality" />
        <property name="threshold" />
    </class>
    
    <class name="SeqLogoParam">
        <id name="hibernateId" column="seqLogoParamId">
            <generator class="increment" />
        </id>        
        <property name="smallSampleCorrection" />
    </class>
    
    <class name="RichConsensus">
        <id name="hibernateId" column="richConsensusId">
            <generator class="increment" />
        </id>
        <property name="bases" type="text"/>
        <property name="conservation" type="text"/>
        <primitive-array name="qualities"> 
            <key column="richConsensusId"/> 
            <index column="IDX"/> 
            <element type="int"/> 
        </primitive-array>        
    </class>    

</hibernate-mapping>
